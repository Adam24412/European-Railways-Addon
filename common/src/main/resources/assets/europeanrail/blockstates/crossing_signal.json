{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "europeanrail:block/crossing_signal"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "europeanrail:block/crossing_signal",
        "y": -90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "europeanrail:block/crossing_signal",
        "y": -180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "europeanrail:block/crossing_signal",
        "y": -270
      }
    }
  ]
}